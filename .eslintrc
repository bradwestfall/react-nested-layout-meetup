{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "jsx-quotes": ["error", "prefer-double"],
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off"
  }
}